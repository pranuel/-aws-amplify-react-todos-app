    "mobx": "^5.13.0",
    "mobx-react": "^6.1.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.0.1",
    "todomvc-app-css": "^2.2.0",
    "todomvc-common": "^1.0.5",
    "typescript": "3.5.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
