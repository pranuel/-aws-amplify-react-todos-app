    "husky": "^3.0.1",
    "lint-staged": "^9.2.1",
    "prettier": "1.18.2",
    "react-test-renderer": "^16.9.0",
    "tslint": "^5.18.0",
    "tslint-react": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
